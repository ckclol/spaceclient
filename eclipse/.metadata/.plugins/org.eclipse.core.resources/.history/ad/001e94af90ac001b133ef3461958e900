package client.cmd.impl;

import client.cmd.CommandExecutor;
import client.cmd.CommandManager;
import client.utils.PrintUtils;
import net.minecraft.client.entity.EntityPlayerSP;
import net.minecraft.util.ChatComponentText;

import java.util.List;

public class Example implements CommandExecutor {
	import java.util.List;

	import net.minecraft.client.entity.EntityPlayerSP;

	/*
	 * THis is a test command, what do you expect it to do? Give you OP?
	 */
	public class CommandTest extends Command {

		public CommandTest() {
			super("test");
		}

		@Override
		public void execute(EntityPlayerSP sender, List<String> args) {
			System.out.println("It worked!");
			for(String s : args) {
				System.out.print(s + ",");
			}
		}

	}
}